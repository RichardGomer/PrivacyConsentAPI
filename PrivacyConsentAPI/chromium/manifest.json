{
  "name": "Privacy Consent Tool",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Indicates your consent to privacy scenarios (such as cookies) to websites",
  "background": {"scripts": ["background.js"]},
 
  "content_scripts":
  	[
	  	{
	  		"matches": ["http://*/*", "https://*/*"],
	  		"js": ["lib/jquery-1.7.2.min.js", "content.js"]
	  	}
  	],
 
  "permissions":
  	[
    	"tabs", "http://*/*", "https://*/*", "storage"
  	]
}